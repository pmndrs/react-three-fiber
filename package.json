{
  "name": "react-three-fiber--root",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "changeset:add": "changeset add",
    "postinstall": "preconstruct dev",
    "build": "preconstruct build",
    "dev": "preconstruct dev",
    "prepare": "husky install",
    "eslint": "eslint --fix packages/**/src/**/*.{ts,tsx}",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false --strict --jsx react",
    "validate": "preconstruct validate",
    "release": "yarn build && yarn changeset publish",
    "vers": "yarn changeset version"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@changesets/changelog-git": "^0.1.9",
    "@changesets/cli": "^2.18.1",
    "@preconstruct/cli": "^2.1.5",
    "@testing-library/react": "^13.0.0-alpha.5",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-native": "0.64.3",
    "@types/react-reconciler": "^0.26.4",
    "@types/react-test-renderer": "^17.0.1",
    "@types/scheduler": "^0.16.2",
    "@types/three": "^0.135.0",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "expo-asset": "^8.4.6",
    "expo-gl": "^11.0.3",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "jest-cli": "^27.4.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "react-native": "0.64.3",
    "react-test-renderer": "^18.0.0-rc.0",
    "regenerator-runtime": "^0.13.9",
    "three": "^0.135.0",
    "three-stdlib": "^2.6.1",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {}
}
